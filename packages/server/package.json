{
  "name": "@promster/server",
  "version": "4.2.8",
  "description": "Server exposing metrics under GET /metrics",
  "main": "dist/index.js",
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "cross-env tsc -p tsconfig.json --outDir dist --declarationDir dist/typings"
  },
  "files": [
    "readme.md",
    "package.json",
    "LICENSE",
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=9",
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdeekens/promster.git"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/promster/issues"
  },
  "homepage": "https://github.com/tdeekens/promster#readme",
  "keywords": [
    "metrics",
    "continousdelivery",
    "prometheus"
  ],
  "dependencies": {
    "@promster/metrics": "^4.1.7"
  },
  "devDependencies": {
    "@types/node": "13.13.4"
  }
}
